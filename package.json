{
  "name": "@figedi/cqrs",
  "version": "1.3.4",
  "description": "Yet another cqrs implementation",
  "license": "MIT",
  "author": "Felix Kaminski",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsup",
    "format": "run-s format:imports format:package-json format:prettier format:eslint",
    "format:eslint": "eslint --cache --ext .ts,.tsx --fix ./ >/dev/null 2>&1 || true",
    "format:imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "format:package-json": "sort-package-json ./package.json",
    "format:prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint --ext ts,tsx src",
    "specs": "vitest run",
    "specs:watch": "vitest"
  },
  "dependencies": {
    "backoff-rxjs": "^7.0.0",
    "fp-ts": "^2.16.11",
    "lodash-es": "^4.17.22",
    "node-schedule": "^2.1.1",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^9.0.1",
    "rxjs": "^7.8.2",
    "serialize-error": "^12.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@types/lodash-es": "^4.17.12",
    "@types/pg": "^8.16.0",
    "@types/node-schedule": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^5.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "pino": "^10.1.0",
    "prettier": "^3.7.4",
    "sort-package-json": "^2.5.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
