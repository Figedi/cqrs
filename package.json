{
  "name": "@figedi/cqrs",
  "version": "1.1.0",
  "description": "Yet another cqrs implementation",
  "license": "MIT",
  "author": "Felix Kaminski",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "run-s format:imports format:package-json format:prettier format:eslint",
    "format:eslint": "eslint --cache --ext .ts,.tsx --fix ./ >/dev/null 2>&1 || true",
    "format:imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "format:package-json": "sort-package-json ./package.json",
    "format:prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint --ext ts,tsx src",
    "specs": "mocha --exit --require ts-node/register/transpile-only './src/**/*.spec.ts'"
  },
  "dependencies": {
    "@figedi/svc": "^0.6.8",
    "fp-ts": "^2.8.5",
    "lodash": "^4.17.20",
    "node-schedule": "^1.3.2",
    "rxjs": "^6.6.3",
    "rxjs-ratelimiter": "^1.0.1",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node-schedule": "^1.3.1",
    "@types/sinon": "^9.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "sinon": "^9.2.1",
    "sort-package-json": "^1.46.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
