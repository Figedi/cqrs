    "node-schedule": "^2.1.0",
    "pg": "^8.7.3",
    "rxjs": "^6.6.3",
    "rxjs-ratelimiter": "^1.0.1",
    "serialize-error": "^7.0.1",
    "typeorm": "^0.3.4",
    "uuid": "^8.3.2",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node-schedule": "^1.3.2",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chai": "^4.3.6",
    "env-cmd": "^10.1.0",
    "eslint": "^8.12.0",
